#FriendlyPermalink($BLOJSOM_TRACKBACK_PLUGIN_BLOG_ENTRY)
<html>
    <head></head>

    <body>
        <p>
            <strong>Trackback Details</strong>
            <br />
            <br />
            Trackback on: <a href="$entryLink">$!BLOJSOM_TRACKBACK_PLUGIN_BLOG_ENTRY.getTitle()</a>
            <br />
            Trackback from: <a href="$!BLOJSOM_TRACKBACK_PLUGIN_TRACKBACK.getUrl()">$!BLOJSOM_TRACKBACK_PLUGIN_TRACKBACK.getBlogName()</a><br />
            Trackback title: $!BLOJSOM_TRACKBACK_PLUGIN_TRACKBACK.getTitle()<br />
            Left on: $!BLOJSOM_TRACKBACK_PLUGIN_TRACKBACK.getTrackbackDate()<br />
            IP Address: $!BLOJSOM_TRACKBACK_PLUGIN_TRACKBACK.getIp()<br />
            Status: $!BLOJSOM_TRACKBACK_PLUGIN_TRACKBACK.getStatus()
            <br />
        </p>
        <p>
            <strong>Trackback Administration</strong>
            <br />
            <br />
            <a href="#BlogAdminURL()/?flavor=admin&action=approve-blog-trackbacks&plugins=edit-blog-entries&blog-entry-id=$BLOJSOM_TRACKBACK_PLUGIN_TRACKBACK.getEntry().getId()&blog-trackback-id=$BLOJSOM_TRACKBACK_PLUGIN_TRACKBACK.getId()">Approve</a> trackback <br />
            <a href="#BlogAdminURL()/?flavor=admin&action=delete-blog-trackbacks&plugins=edit-blog-entries&blog-entry-id=$BLOJSOM_TRACKBACK_PLUGIN_TRACKBACK.getEntry().getId()&blog-trackback-id=$BLOJSOM_TRACKBACK_PLUGIN_TRACKBACK.getId()">Delete</a> trackback <br />
            <a href="#BlogAdminURL()/?flavor=admin&action=edit-blog-entry&plugins=edit-blog-entries&blog-category-id=$BLOJSOM_TRACKBACK_PLUGIN_TRACKBACK.getEntry().getBlogCategory().getId()&blog-entry-id=$BLOJSOM_TRACKBACK_PLUGIN_TRACKBACK.getEntry().getId()">Edit</a> entry <br />
        </p>
        <p>
            <strong>Trackback</strong>
            <br />
            <br />
            $!BLOJSOM_TRACKBACK_PLUGIN_TRACKBACK.getExcerpt()
        </p>
    </body>
</html>